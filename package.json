{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production postcss styles/tailwind.css --o _site/style.css && cleancss _site/style.css -o _site/style.css",
    "build:inline-css": "NODE_ENV=production postcss styles/tailwind.css --o _includes/_tmp/style.css && ELEVENTY_PRODUCTION=true eleventy",
    "watch": "eleventy --watch",
    "dev": "eleventy --serve & postcss styles/tailwind.css --o _tmp/style.css --watch",
    "start": "eleventy --serve",
    "debug": "DEBUG=* eleventy"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-img": "^0.7.8",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "alpinejs": "^2.8.0",
    "autoprefixer": "^10.2.4",
    "clean-css": "^5.0.1",
    "clean-css-cli": "^4.3.0",
    "dayjs": "^1.10.4",
    "eleventy-plugin-toc": "^1.1.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  }
}
